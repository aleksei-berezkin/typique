{
  "name": "typique",
  "version": "1.0.0",
  "description": "Bundler-agnostic, zero-runtime CSS-in-TS â€” powered by a TypeScript plugin",
  "license": "ISC",
  "author": "Aleksei Berezkin",
  "keywords": [
    "bundler-agnostic",
    "css",
    "css-in-ts",
    "framework-agnostic",
    "ide-tooling",
    "typescript",
    "typescript-plugin",
    "zero-runtime"
  ],
  "main": "./esm/index.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    },
    "./util": {
      "types": "./esm/util/index.d.ts",
      "import": "./esm/util/index.js",
      "require": "./cjs/util/index.js"
    }
  },
  "bin": {
    "typique": "bin/typique.mjs"
  },
  "files": [
    "bin",
    "cjs",
    "esm",
    "ts-plugin",
    "!*.tsbuildinfo",
    "!*.test.*"
  ],
  "dependencies": {
    "csstype": "^3.1.3",
    "minimatch": "^10.1.1"
  }
}
