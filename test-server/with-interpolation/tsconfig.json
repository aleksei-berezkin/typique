{
  "compilerOptions": {
    "jsx": "preserve",
    "module": "nodenext",
    "strict": true,
    "plugins": [
      {
        "name": "typique/ts-plugin",
        "namingOptions": {
          "pattern": "${contextName}_${s}",
        },
        "output": {
          "sourceFileNames": true
        }
      }
    ]
  }
}
