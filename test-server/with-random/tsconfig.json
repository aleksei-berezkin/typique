{
  "compilerOptions": {
    "module": "nodenext",
    "strict": true,
    "plugins": [
      {
        "name": "typique/ts-plugin",
        "namingOptions": {
          "pattern": "${contextName}_${random(8, 3)}",
        },
        "output": {
          "sourceFileNames": true
        }
      }
    ]
  }
}
